#11654(아스키 코드 변환)
a = input()
print(ord(a))

#11720(숫자의 합)
num_count = input()
number = input()
result = 0
for i in number:
    result += int(i)
print(result)

#2675
test_num = int(input())
for _ in range(test_num):
    cnt, str_test = input().split()
    result = ''
    for i in str_test:
        result += i * int(cnt)
    print(result)
