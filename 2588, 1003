#2588
# A, B = input().strip().split( )
A = input()
B = input()

B = B[::-1]
result = 0
x = 0
for i in B:
    print(int(A)*int(i))
    result += (int(A)*int(i))*10**x
    x += 1
print(result)

#1003 
zero = [1, 0]
one = [0, 1]

for i in range(2,41):
    zero.append(zero[i-1]+zero[i-2])
    one.append(one[i-1]+one[i-2])

test = int(input())
for x in range(test):
    num = int(input())
    print(zero[num], one[num])
