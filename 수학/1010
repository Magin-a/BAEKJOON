#1010 다리놓기
my_num = int(input())
def result(x, y):
    x= int(x)
    y= int(y)
    fel1 = 1
    fel2 = 1
    fel3 = 1

    for i in range(y):
        fel1 *= y-i 
        
    for j in range(x):
        fel2 *= x-j
    for z in range(y-x):
        fel3 *= (y-x)-z 

    

    rel = fel1//(fel2*fel3)
    return rel



for i in range(int(my_num)):
    west, east =  input().strip().split( )
    print(result(west, east))
